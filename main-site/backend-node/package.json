{
  "name": "thesocialmask-backend-node",
  "version": "0.1.0",
  "description": "Backend service for TheSocialMask smart accounts, swaps and device management.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && npm start\"",
    "dev:ts": "tsx watch src/server.ts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "event-listener": "node src/event-listener.js",
    "event-listener:dev": "nodemon --watch src/event-listener.js src/event-listener.js",
    "test:event-listener": "node test-event-listener.js",
    "setup:event-listener": "node setup-event-listener.js"
  },
  "dependencies": {
    "@gelatonetwork/relay-sdk": "^5.6.0",
    "@simplewebauthn/server": "^13.2.2",
    "@types/ws": "^8.5.10",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "gun": "^0.2020.1241",
    "helmet": "^7.1.0",
    "jose": "^5.2.0",
    "mysql2": "^3.10.0",
    "pino": "^9.1.0",
    "pino-http": "^10.2.0",
    "qs": "^6.12.1",
    "secrets.js-grempe": "^2.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^20.12.7",
    "@types/qs": "^6.9.11",
    "@types/uuid": "^9.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.0",
    "pino-pretty": "^11.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
